{
  "Logging": {
    "LogLevel": {
      "Default": "Information"
    }
  },
  "AllowedHosts": "*",
  "YoutubeConfiguration": {
    "ExecutablePath": "/app/youtube-dl/youtube-dl",
    "DownloadPath": "/tmp"
  },
  "Metrics": {
    "Enabled": true,
    "Port": 9000,
    "Uri": "/metrics"
  },
  "DataRetention": {
    "Enabled": true,
    "DeleteFilesAfterMinutesElapsed": 5
  }
}
